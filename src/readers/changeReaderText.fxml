<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="335.0" prefWidth="620.0" style="-fx-background-color: lightgray;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="readers.changeReaderTextController">
   <children>
      <Label layoutX="93.0" layoutY="29.0" text="Change Reader Text">
         <font>
            <Font size="49.0" />
         </font>
      </Label>
      <Label layoutX="93.0" layoutY="100.0" text="This change will be applied to the currently selected reader (Location)">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="readerTextChange" layoutX="78.0" layoutY="203.0" prefHeight="25.0" prefWidth="465.0" promptText="Provide a new Location name ">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="save" layoutX="93.0" layoutY="260.0" mnemonicParsing="false" onAction="#BtnSave" prefHeight="51.0" prefWidth="156.0" text="Save">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="cancel" layoutX="366.0" layoutY="260.0" mnemonicParsing="false" onAction="#BtnCancel" prefHeight="51.0" prefWidth="156.0" text="Cancel">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Label layoutX="78.0" layoutY="137.0" text="Location name = " />
      <Label fx:id="nameEquals" layoutX="90.0" layoutY="154.0" prefHeight="17.0" prefWidth="441.0" text="Label">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Label>
   </children>
</AnchorPane>
